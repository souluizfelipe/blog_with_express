<!DOCTYPE html>
<html lang="en">
  <head>
    <%- include('partials/head') %> 
  </head>
<body>
 <%- include('partials/header') %> 

  <section class="main-section-post">
    <% posts.forEach(post => { %>

      <article class="full-post">
        <h1 class="post-title"><%= post.title %></h1>
        <p class="post-text">
          <%= post.text %>
        </p>
      </article>   
    
      <% }) %>
  </section>

 <%- include('partials/footer') %>
</body>
</html>